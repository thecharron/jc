{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(.*),
      "dest": "/api/index.php",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    }
  ]
}
